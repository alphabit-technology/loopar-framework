{
  "name": "loopar-framework",
  "version": "2.0.0",
  "type": "module",
  "engines": {
    "node": ">=21.0.0"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "preinstall": "node bin/installer-clean.js",
    "dev": "cross-env NODE_ENV=development && node node_modules/loopar",
    "start": "cross-env NODE_ENV=production && node node_modules/loopar",
    "start-backend": "node node_modules/loopar",
    "start-vite": "vite clean && vite",
    "build": "node bin/build-clean.js && npm-run-all build:client build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.jsx"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "axios": "^1.7.9",
    "better-sqlite3": "^11.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "date-fns": "4.1.0",
    "dayjs": "1.11.13",
    "decamelize": "^6.0.0",
    "easymde": "^2.18.0",
    "eventemitter3": "^5.0.1",
    "express-session": "^1.17.3",
    "express-useragent": "^1.0.15",
    "file-type": "^18.5.0",
    "flatted": "^3.3.2",
    "inflection": "^3.0.0",
    "js-cookie": "^3.0.5",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "loopar": "file:.loopar",
    "method-override": "^3.0.0",
    "mlly": "^1.7.3",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.67.3",
    "particles.js": "^2.0.0",
    "pathe": "^1.1.2",
    "pluralize": "^8.0.0",
    "sequelize": "^6.37.4",
    "sha1": "^1.1.1",
    "sharp": "^0.33.5",
    "simple-git": "^3.27.0",
    "sirv": "^2.0.4",
    "sonner": "^1.7.1",
    "sqlite3": "^5.1.7",
    "stripe": "^17.3.1",
    "tedious": "^18.6.1",
    "typeface-inter": "^3.18.1",
    "typeface-jetbrains-mono": "^2.0.1",
    "universal-cookie": "^7.0.1",
    "uuid": "11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "autoprefixer": "^10.4.17",
    "compression": "^1.7.5",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "serve-static": "^1.16.2",
    "terser": "^5.36.0"
  }
}
