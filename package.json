{
  "name": "loopar-framework",
  "version": "1.1.5",
  "private": false,
  "type": "module",
  "engines": {
    "node": ">=21.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development && node node_modules/loopar",
    "start": "cross-env NODE_ENV=production && node node_modules/loopar",
    "start-backend": "node node_modules/loopar",
    "start-vite": "vite clean && vite",
    "build": "rimraf dist && npm-run-all build:client build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.jsx"
  },
  "_moduleAliases": {
    "@root": ".",
    "@loopar": "apps/loopar"
  },
  "dependencies": {
    "@radix-ui/react-scroll-area": "^1.2.1",
    "loopar": "file:.loopar",
    "stripe": "^17.3.1",
    "tailwindcss": "^3.4.15",
    "vite-plugin-compression": "^0.5.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "^20.10.4",
    "@types/nprogress": "0.2.3",
    "@types/numeral": "2.0.5",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.17",
    "compression": "^1.7.5",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "daisyui": "^4.6.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "mysql": "^2.18.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^2.8.8",
    "react": "v19-beta",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-critical": "^1.0.13",
    "rollup-plugin-visualizer": "^5.12.0",
    "serve-static": "^1.16.2",
    "terser": "^5.36.0",
    "typescript": "^5.5.4",
    "vite": "^6.0.1",
    "vite-plugin-express": "^1.0.3",
    "vite-plugin-preload": "^0.4.2"
  }
}
